{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "pip install -r requirements.txt",
        "buildCommand": "streamlit run app.py --server.port=3000"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}